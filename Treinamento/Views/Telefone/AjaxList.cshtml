@model IEnumerable<Treinamento.Core.Model.Telefone>

<p>
    <a href="javascript:abrirDialogTelefone();">Novo</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>@Html.DisplayNameFor(q => q.Numero)</th>
            <th></th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {
            <tr>
                <td>@item.Numero</td>
                <td>
                    <a href="javascript:alterarTelefone(@item.Id, '@item.Numero');">Alterar</a>
                    <a href="javascript:excluirTelefone(@item.Id);">Excluir</a>
                </td>
            </tr>
        }
    </tbody>

</table>